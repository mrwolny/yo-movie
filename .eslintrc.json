{
  "extends": ["airbnb"],
  "env": {
      "browser": true,
      "node": true,
      "jest": true,
      "es6": true
  },
  "parser": "babel-eslint",
  "rules": {
    "no-underscore-dangle": "off",
    "react/jsx-filename-extension": ["error", { "extensions": [".js"] }],
    "comma-dangle": ["error", {
      "arrays": "always-multiline",
      "objects": "always-multiline",
      "imports": "never",
      "exports": "never",
      "functions": "never"
    }]
  }
}
